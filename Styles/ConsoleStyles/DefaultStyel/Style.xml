<?xml version="1.0" encoding="utf-8"?>
<ConsoleStyle>
  <Info>
    <StyleName>DefaultStyle</StyleName>
    <DefaultLogStyle>DefaultStyle</DefaultLogStyle>
  </Info>

  <GUIStyle>
    <Box>CN Box</Box>
    <MiniButton>ToolbarButton</MiniButton>
    <Toolbar>Toolbar</Toolbar>
    <LogStyle>CN EntryInfo</LogStyle>
    <WarningStyle>CN EntryWarn</WarningStyle>
    <ErrorStyle>CN EntryError</ErrorStyle>
    <IconLogStyle>CN EntryInfoIcon</IconLogStyle>
    <IconWarningStyle>CN EntryWarnIcon</IconWarningStyle>
    <IconErrorStyle>CN EntryErrorIcon</IconErrorStyle>
    <EvenBackground>CN EntryBackEven</EvenBackground>
    <OddBackground>CN EntryBackodd</OddBackground>
    <MessageStyle>
      <Name>CN Message</Name>
      <normal>
        <textColor>1,1,1,1</textColor>
      </normal>
      <onNormal>
        <textColor>1,1,1,1</textColor>
      </onNormal>
      <FongSize>15</FongSize>
      <Padding>0,5,0,0</Padding>
    </MessageStyle>
    <StatusError>CN StatusError</StatusError>
    <StatusWarn>CN StatusWarn</StatusWarn>
    <StatusLog>CN StatusInfo</StatusLog>
    <CountBadge>CN CountBadge</CountBadge>
    <LogSmallStyle>CN EntryInfoSmall</LogSmallStyle>
    <WarningSmallStyle>CN EntryWarnSmall</WarningSmallStyle>
    <ErrorSmallStyle>CN EntryErrorSmall</ErrorSmallStyle>
    <IconLogSmallStyle>CN EntryInfoIconSmall</IconLogSmallStyle>
    <IconWarningSmallStyle>CN EntryWarnIconSmall</IconWarningSmallStyle>
    <IconErrorSmallStyle>CN EntryErrorIconSmall</IconErrorSmallStyle>
  </GUIStyle>

  <StackTraceIgnore>
    <File>
      <File>ConsoleTiny/Scripts</File>

    </File>
    <NameSpace>
      <NameSpace>ConsoleTiny</NameSpace>
      <NameSpace>GSFramework.Dev</NameSpace>
    </NameSpace>
  </StackTraceIgnore>

  <LogType>
    <Type>Log</Type>
    <Type>MainParameter</Type>
    <Type>Examine</Type>
    <Type>Warning</Type>
    <Type>Error</Type>
    <Type>Assert</Type>
    <Type>Disastrous</Type>
  </LogType>

  <LogGroup>
    <Group>System</Group>
    <Group>TMP</Group>
    <Group>IOC</Group>
  </LogGroup>
</ConsoleStyle>